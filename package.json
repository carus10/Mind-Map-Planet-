{
  "name": "mindmap-world",
  "version": "1.0.0",
  "description": "Obsidian vault visualizer as an interactive 2D world map",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "dist": "electron-vite build && electron-builder",
    "dist:win": "electron-vite build && electron-builder --win",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "build": {
    "appId": "com.mindmapworld.app",
    "productName": "Mind Map Planet",
    "directories": {
      "output": "release"
    },
    "files": [
      "out/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@turf/bbox": "^7.3.4",
    "@turf/helpers": "^7.3.4",
    "@turf/intersect": "^7.3.4",
    "@turf/voronoi": "^7.3.4",
    "@types/d3-delaunay": "^6.0.4",
    "@types/d3-geo": "^3.1.0",
    "@types/topojson-client": "^3.1.5",
    "d3-delaunay": "^6.0.4",
    "d3-geo": "^3.1.1",
    "d3-polygon": "^3.0.1",
    "electron-store": "^8.2.0",
    "sane-topojson": "^4.0.0",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2",
    "world-geojson": "^3.4.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@nicolo-ribaudo/chokidar-2": "^2.1.8-no-fsevents.3",
    "@testing-library/react": "^14.3.1",
    "@types/d3-polygon": "^3.0.2",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "fast-check": "^3.20.0",
    "jsdom": "^24.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}